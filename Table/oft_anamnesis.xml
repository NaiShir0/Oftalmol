<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
    </column>
    <column>
        <name>idexpediente</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>name</name>           <!-- Anamnesis -->
        <type>text</type>
        <null>NO</null>
    </column>

    <constraint>
        <name>oft_anamnesis_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_oft_anamnesis_expedientes</name>
        <type>FOREIGN KEY (idexpediente) REFERENCES oft_expedientes (id) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>