<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>creationDate</name>
        <type>datetime</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
    </column>
    <column>
        <name>idSpeciality</name>
        <type>int</type>
        <null>NO</null>
    </column>
    <column>
        <name>idTestType</name>
        <type>int</type>
        <null>NO</null>
    </column>
    <column>
        <name>idExpedient</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>description</name>           <!-- Anamnesis -->
        <type>text</type>
        <null>NO</null>
    </column>

    <constraint>
        <name>oft_notes_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>fk_oft_notes_expedient</name>
        <type>FOREIGN KEY (idExpedient) REFERENCES oft_expedients (id) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>