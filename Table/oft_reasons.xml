<table>
    <column>
        <name>idReason</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>reasonName</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>

    <constraint>
        <name>oft_reasons_pkey</name>
        <type>PRIMARY KEY (idReason)</type>
    </constraint>
</table>