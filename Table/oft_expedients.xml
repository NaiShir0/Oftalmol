<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>releaseDate</name>
        <type>date</type>
    </column>
    <column>
        <name>creationDate</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>modificationDate</name>
        <type>datetime</type>
        <null>NO</null>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
    </column>
    <column>
        <name>idReason</name>
        <type>integer</type>
        <null>NO</null>
    </column>

    <constraint>
        <name>oft_expedients_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>fk_oft_expedients_clients</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>fk_oft_expedients_reasons</name>
        <type>FOREIGN KEY (idreason) REFERENCES oft_reasons (id) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
</table>
