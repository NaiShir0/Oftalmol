<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>alta</name>
        <type>date</type>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>fechamodificacion</name>
        <type>datetime</type>
        <null>NO</null>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
    </column>
    <column>
        <name>idmotivo</name>
        <type>integer</type>
        <null>NO</null>
    </column>

    <constraint>
        <name>ofc_expedientes_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_ofc_expedientes_clientes</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_ofc_expedientes_motivos</name>
        <type>FOREIGN KEY (idmotivo) REFERENCES oft_motivos (id) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
</table>
