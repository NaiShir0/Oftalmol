<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <column name="code" display="none" order="100">
            <widget type="text" fieldname="id" />
        </column>
        <column name="date-expedient" order="110" display="center">
            <widget type="date" fieldname="fecha"/>
        </column>
        <column name="datemod" order="120" display="center">
            <widget type="date" fieldname="fechamodificacion"/>
        </column>
        <column name="patient" order="120">
            <widget type="text" fieldname="nombre"/>
        </column>
        <column name="reason" title="reason-consultation" order="130">
            <widget type="text" fieldname="motivo" />
        </column>
        <column name="mobile" order="140" display="center">
            <widget type="text" fieldname="telefono1"/>
        </column>
        <column name="phone" display="none" order="150">
            <widget type="text" fieldname="telefono2"/>
        </column>
        <column name="email" display="none" order="160">
            <widget type="text" fieldname="email"/>
        </column>
        <column name="date-birth" order="170" display="center">
            <widget type="text" fieldname="nacimiento"/>
        </column>
        <column name="discharge" order="180" display="center">
            <widget type="date" fieldname="alta" />
        </column>
    </columns>
    <rows>
        <row type="status" fieldname="alta">
            <option title="closed" color="info">&gt;0</option>
        </row>
    </rows>
</view>