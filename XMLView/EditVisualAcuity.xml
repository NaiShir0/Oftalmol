            <column name="OSfarSCnote" order="150" numcolumns="3" title="left-eye">
                <widget type="text" fieldname="OSfarSCnote"/>
            </column>
            <!-- Campo para observaciones sin compensacion -->  
            <column name="FarSCnote" order="160" numcolumns="4" title="observations">
                <widget type="textarea" fieldname="FarSCnote" rows="1"/>
            </column>
        </group>
        
        
