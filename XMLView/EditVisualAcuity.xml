<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <group name="header" numcolumns="12">
            <column name="id" order="100" display="none" numcolumns="1" >
                <widget type="text" fieldname="id" />
            </column>
            <column name="idSpeciality" order="110" display="none" numcolumns="1">
                <widget type="text" fieldname="idSpeciality" />
            </column>
            <column name="idExpedient" order="120" display="none" numcolumns="1">
