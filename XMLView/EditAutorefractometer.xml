<view>
    <columns>
        <group name="header" numcolumns="12">
            <column name="id" order="100" display="none" numcolumns="1" >
                <widget type="text" fieldname="id" />
            </column>
            <column name="idSpeciality" order="110" display="none" numcolumns="1">
                <widget type="text" fieldname="idSpeciality" />
            </column>
            <column name="idExpedient" order="120" display="none" numcolumns="1">
                <widget type="text" fieldname="idExpedient" />
            </column>
            <column name="idTestType" order="130" display="none" numcolumns="1">
                <widget type="text" fieldname="idTestType" />
            </column>
            <column name="creationDate" order="140" numcolumns="2">
                <widget type="date" fieldname="creationDate" />
            </column>
            <column name="creationTime" order="150" numcolumns="2">
                <widget type="time" fieldname="creationTime" />
            </column>
            <column name="modificationDate" order="160" numcolumns="2">
                <widget type="date" fieldname="modificationDate" />
            </column>
            <column name="modificationTime" order="170" numcolumns="2">
                <widget type="time" fieldname="modificationTime" />
            </column>
        </group>
        
        <group name="withoutCycle" numcolumns="6" title="withoutCycle">    
            <!-- Campos para el lejos del OD -->    
            <column name="ODfarSphere" order="150" numcolumns="4">
                <widget type="text" fieldname="ODfarSphere" />
            </column>
            <column name="ODfarCylinder" order="160" numcolumns="4">
                <widget type="text" fieldname="ODfarCylinder" />
            </column>
            <column name="ODfarAxis" order="170" numcolumns="4">
                <widget type="text" fieldname="ODfarAxis" />
            </column>
            <column name="ODfarNote" order="180" numcolumns="12">
                <widget type="text" fieldname="ODfarNote" />
            </column>
            
            <!-- Campos para el lejos del OS -->    
            <column name="OSfarSphere" order="190" numcolumns="4">
                <widget type="text" fieldname="OSfarSphere" />
            </column>
            <column name="OSfarCylinder" order="200" numcolumns="4">
                <widget type="text" fieldname="OSfarCylinder" />
            </column>
            <column name="OSfarAxis" order="210" numcolumns="4">
                <widget type="text" fieldname="OSfarAxis" />
            </column>
            <column name="OSfarNote" order="220" numcolumns="12">
                <widget type="text" fieldname="OSfarNote" />
            </column>
            
            <column name="farNote" order="230" numcolumns="12" rows="1">
                <widget type="textarea" fieldname="farNote"/>
            </column>
        </group>
        
        
        <group name="withCycle" numcolumns="6" title="withCycle">    
            <!-- Campos para el cerca del OD -->    
            <column name="ODcloseSphere" order="240" numcolumns="4">
                <widget type="text" fieldname="ODcloseSphere" />
            </column>
            <column name="ODcloseCylinder" order="250" numcolumns="4">
                <widget type="text" fieldname="ODcloseCylinder" />
            </column>
            <column name="ODcloseAxis" order="260" numcolumns="4">
                <widget type="text" fieldname="ODcloseAxis" />
            </column>
            <column name="ODcloseNote" order="270" numcolumns="12">
                <widget type="text" fieldname="ODcloseNote" />
            </column>
            
            <!-- Campos para el cerca del OS -->    
            <column name="OScloseSphere" order="280" numcolumns="4">
                <widget type="text" fieldname="OScloseSphere" />
            </column>
            <column name="OScloseCylinder" order="290" numcolumns="4">
                <widget type="text" fieldname="OScloseCylinder" />
            </column>
            <column name="OScloseAxis" order="300" numcolumns="4">
                <widget type="text" fieldname="OScloseAxis" />
            </column>
            <column name="OScloseNote" order="310" numcolumns="12">
                <widget type="text" fieldname="OScloseNote" />
            </column>
            
            <column name="closeNote" order="320" numcolumns="12" rows="1">
                <widget type="textarea" fieldname="closeNote"/>
            </column>
        </group>
        
        <group title="footer">
            <column name="generalNote" order="40" numcolumns="12" >
                <widget type="text" fieldname="generalNote"/>
            </column>
            <column name="profesionalNote" order="410" numcolumns="12" >
                <widget type="textarea" fieldname="profesionalNote" rows="1"/>
            </column>
        </group>
    </columns>
</view>